
generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id  Int  @id @default(autoincrement())
  email  String @unique
  password Int
  script Script[]
}

model Script{
  id  Int @id @default(autoincrement())
  userPrompt  String
  script String
  videoUrl String
  user User  @relation(fields: [userId],references: [id])
  userId Int
}